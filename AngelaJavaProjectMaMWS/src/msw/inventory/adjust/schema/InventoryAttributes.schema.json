{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"properties": {
		"InventoryAttributes": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"AttributeName": {
						"type": "string"
					},
					"AttributeValue": {
						"type": "string"
					},
					"AttributeUom": {
						"type": "string"
					}
				},
				"required": [
					"AttributeName",
					"AttributeValue",
					"AttributeUom"
				]
			}
		}
	},
	"required": [
		"InventoryAttributes"
	]
}
